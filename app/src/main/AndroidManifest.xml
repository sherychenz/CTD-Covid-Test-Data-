<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ctdcovidtestdata">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CTDCovidTestData">
        <activity android:name=".Update_Result_Activity"></activity>
        <activity android:name=".Record_Patient_Activity" />
        <activity android:name=".Tester_Menu_Activity" />
        <activity android:name=".ManageTestKitStockActivity" />
        <activity android:name=".RecordTesterActivity" />
        <activity android:name=".RegisterCenterMenuActivity" />
        <activity android:name=".TestCenterManagerMenuActivity" />
        <activity android:name=".NewTestKitActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>